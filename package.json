{
  "name": "CerealVis",
  "version": "0.1.0",
  "private": true,
  "description": "Electron App made to read in and visualize data coming from a serial connection.",
  "author": "Brian Crook, Tran Nguyen, Emilio Quintana, Brenden Roland, Logan Friend, Ero Doce",
  "build": {
    "appId": "CerealVis",
    "files": [
      "src/**/*",
      "public/**/*",
      "public/images/logos/*",
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "win": {
      "target": "portable"
    },
    "mac": {}
  },
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.48",
    "@material-ui/styles": "^4.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "chart.js": "^2.9.3",
    "chartjs-plugin-streaming": "^1.8.0",
    "cross-env": "^7.0.0",
    "electron-is-dev": "^1.1.0",
    "install": "^0.13.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "npm": "^6.13.7",
    "p5": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "react-perfect-scrollbar": "^1.5.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "reactjs-popup": "^1.5.0",
    "serialport": "^8.0.7"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^13.6.6",
    "electron-builder": "^22.3.2",
    "electron-rebuild": "^1.10.0",
    "wait-on": "^4.0.0",
    "typescript": "^3.5.1"
  },
  "scripts": {
    "rebuild-mac": "$(npm bin)/electron-rebuild",
    "rebuild-win": ".\\node_modules\\.bin\\electron-rebuild.cmd",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
