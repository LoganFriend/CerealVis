{
  "name": "CerealVis",
  "version": "0.1.0",
  "description": "A Desktop application to read in and display readings from an emg sensor.",
  "main": "public/main.js",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.1",
    "serialport": "^8.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "rebuild": ".\\node_modules\\.bin\\electron-rebuild.cmd",
    "electron-start": "electron .",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder --win -c.extraMetadata.main=build/main.js"
  },
  "build": {
    "appId": "nonExistentID",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": "https://github.com/LoganFriend/CerealVis",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "electron-rebuild": "^1.10.0",
    "typescript": "^3.7.5"
  }
}
